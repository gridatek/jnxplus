{
  "name": "jnxplus-smoke",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "testing-projects/smoke/jnxplus-smoke/src",
  "targets": {
    "smoke": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "testing-projects/smoke/jnxplus-smoke/jest.config.js",
        "passWithNoTests": true,
        "runInBand": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["testing-projects/smoke/jnxplus-smoke/**/*.ts"]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["nx-gradle", "nx-maven"]
}
